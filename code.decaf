class Program
{
    int f(int a, int b) {
        int i;
        int number;
        number = 1;
        for i = 0, i < b
        {
            number = number * a;
            i = i + 1;
        }

        return number;
    }
    void main()
    {
        int x;
        x = f(2, 2) + 0;
        callout("Resut: ");
        callout(x);
    }
}